  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:full": "node start-goryl.js",
    "validate:env": "node scripts/validate-build-env.js",
    "build": "node scripts/validate-build-env.js && export NODE_OPTIONS=--max-old-space-size=8192 && next build",
    "build:firebase": "set NODE_OPTIONS=--max-old-space-size=8192 && next build && node copy-next-to-functions.js",
    "deploy": "npm run build:firebase && firebase deploy",
    "deploy:fix": "npm run build:firebase && firebase deploy --only functions && firebase deploy --only hosting",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:e2e": "playwright test",
    "firebase:emulators": "firebase emulators:start",
    "firebase:functions": "firebase deploy --only functions",
    "firebase:hosting": "firebase deploy --only hosting",
    "firebase:storage": "node scripts/deploy-storage-rules.js",
    "firebase:indexes": "tsx src/scripts/deployIndexes.ts",
    "firebase:fix-error": "node copy-next-to-functions.js && npm run firebase:functions",
    "migrate:firestore-to-dynamodb": "tsx scripts/migrate-firestore-to-dynamodb.ts",
    "migrate:firestore": "npm run migrate:firestore-to-dynamodb",
    "check:env": "node scripts/check-env.js",
    "check:env:format": "node scripts/check-env-format.js",
    "fix:env:local": "node scripts/fix-env-local.js",
    "verify:cognito": "node scripts/verify-cognito-env.js",
    "fix:env": "node scripts/fix-env-encoding.js",
    "rebuild:env": "node scripts/rebuild-env.js"
  },
